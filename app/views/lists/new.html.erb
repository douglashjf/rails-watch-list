<div class="container p-3">
  <h2>Create new list</h2>
  <%= simple_form_for @list do |f| %>
    <%= f.input :name, placeholder: "any genre you want!" %>
    <%= f.button :submit, class: "btn btn-success" %>
    <%= f.input :photo, as: :file %>
  <% end %>

    <%= link_to '< All lists', lists_path %>
</div>
